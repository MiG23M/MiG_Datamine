            "ButtonImg": {            }
          },
          "Button_text": {
            "id": "btn_fav",
            "text": "#preloaderSettings/trackProgress",
            "btnName": "A",
            "on_click": "onToggleFav",
            "visualStyle": "secondary",
            "buttonWink": {            },
            "ButtonImg": {            }
          },
          "Button_text": {
            "id": "btn_buy",
            "text": "#mainmenu/btnOrder",
            "btnName": "A",
            "on_click": "onBuy",
            "visualStyle": "purchase",
            "buttonWink": {            },
            "buttonGlance": {            },
            "ButtonImg": {            }
          }
        }
      }
    }
  }
