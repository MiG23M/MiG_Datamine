{
  "root": {
    "behaviour": "button",
    "class": "button",
    "blur": {    },
    "blur_foreground": {    },
    "frame": {
      "id": "wnd_frame",
      "size": "1.2@sf, 1@maxWindowHeight - 0.1@sh",
      "class": "wndNav",
      "isCenteredUnderLogo": "yes",
      "css-hier-invalidate": "yes",
      "frame_header": {
        "css-hier-invalidate": "yes",
        "activeText": {
          "id": "wnd_title",
          "caption": "yes",
          "text": "#options/profilePilot"
        },
        "Button_close": {
          "id": "btn_back"
        }
      },
      "tdiv": {
        "id": "images_list",
