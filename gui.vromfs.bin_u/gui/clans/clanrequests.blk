{
  "root": {
    "type": "big",
    "blur": {    },
    "blur_foreground": {    },
    "frame": {
      "size": "0.8*@scrn_tgt, 0.5@scrn_tgt",
      "flow": "vertical",
      "class": "wndNav",
      "isCenteredUnderLogo": "yes",
      "frame_header": {
        "activeText": {
          "id": "clan_title_table",
          "caption": "yes",
          "text": "#clan/candidatesList"
        },
        "Button_close": {
          "id": "btn_back"
        }
      },
      "table": {
        "id": "candidatesList",
        "width": "pw",
        "height": "fh",
        "padding-bottom": "0.02*@scrn_tgt",
        "class": "lbTable",
        "selfFocusBorder": "yes",
        "behavior": "PosNavigator",
        "text-valign": "center",
        "text-halign": "center",
        "navigatorShortcuts": "yes",
        "on_click": "onSelect",
        "_on_r_click": "onUserRClick",
        "on_dbl_click": "onUserCard"
      },
      "navBar": {
        "navLeft": {
          "Button_text": {
            "id": "btn_reject",
            "text": "#clan/requestReject",
            "display": "hide",
            "_on_click": "onRequestReject",
            "ButtonImg": {            }
          },
          "Button_text": {
            "id": "btn_user_options",
            "text": "#mainmenu/btnUserAction",
            "btnName": "X",
            "display": "hide",
            "_on_click": "onUserAction",
            "ButtonImg": {            }
          }
        },
        "navMiddle": {
          "id": "paginator_place"
        },
        "navRight": {
          "Button_text": {
            "id": "btn_removeBlacklist",
            "text": "#msgbox/btn_delete",
            "_on_click": "onDeleteFromBlacklist",
            "display": "hide",
            "btnName": "A",
            "ButtonImg": {            }
          },
          "Button_text": {
            "id": "btn_approve",
            "visualStyle": "secondary",
            "text": "#clan/requestApprove",
            "display": "hide",
            "_on_click": "onRequestApprove",
            "ButtonImg": {            },
            "buttonWink": {            }
          }
        }
      }
    }
  }
}