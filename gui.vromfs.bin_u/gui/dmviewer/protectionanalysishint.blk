{
  "tdiv": {
    "id": "target_cursor",
    "behavior": "Timer",
    "timer_handler_func": "onTargetingCursorTimer",
    "size": "48@sf/@pf, 48@sf/@pf",
    "left": "sw/2-w/2",
    "top": "sh/2-h/2",
    "position": "root",
    "background-image": "#ui/gameuiskin#protection_analysis_target.svg",
    "background-svg-size": "48@sf/@pf, 48@sf/@pf",
    "background-color": "@minorTextColor"
  },
  "tooltipObj": {
    "id": "dmviewer_hint",
    "behavior": "Timer",
    "timer_handler_func": "onDMViewerHintTimer",
    "position": "root",
    "pos": "sw/2-w/2, 70%sh-h/2",
    "style": "padding:16@sf/@pf; color-factor:192;",
    "display": "hide",
    "flow": "vertical",
    "input-transparent": "yes",
    "order-popup": "yes",
    "textareaNoTab": {
      "id": "dmviewer_title",
      "overlayTextColor": "active",
      "text": "",
      "input-transparent": "yes"
    },
    "textareaNoTab": {
      "id": "dmviewer_desc",
      "position": "relative",
      "margin-top": "8@sf/@pf",
      "hideEmptyText": "yes",
      "text": "",
      "input-transparent": "yes"
    },
    "tdiv": {
      "id": "dmviewer_ext_hint",
      "flow": "vertical",
      "position": "relative",
      "width": "0.5@sf",
      "total-input-transparent": "yes",
      "display": "hide",
      "rowSeparator": {
        "margin": "0, @blockInterval"
      },
      "textareaNoTab": {
        "position": "relative",
        "margin-top": "@blockInterval",
        "overlayTextColor": "active",
        "text": "#xray_ext_hint/title"
      },
      "textareaNoTab": {
        "id": "dmviewer_ext_hint_desc",
        "position": "relative",
        "margin-top": "@blockInterval",
        "hideEmptyText": "yes",
        "text": "",
        "smallFont": "yes",
        "width": "pw"
      },
      "img": {
        "id": "dmviewer_ext_hint_icon",
        "position": "relative",
        "margin-top": "2@blockInterval",
        "size": "@dIco, @dIco",
        "background-image": ""
      },
      "loadingHint": {
        "id": "dmviewer_ext_hint_shortcut",
        "behaviour": "bhvHint",
        "position": "relative",
        "width": "pw",
        "isXrayHint": "yes",
        "smallFont": "yes",
        "isWrapInRowAllowed": "yes",
        "flow-align": "left"
      }
    }
  }
}