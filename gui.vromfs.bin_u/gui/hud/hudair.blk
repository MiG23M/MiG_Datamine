{
  "tacticalMap": {
    "id": "hud_air_tactical_map",
    "size": "@sizeTacticalMap, @sizeTacticalMap",
    "position": "absolute",
    "pos": "pw - w, ph - h"
  },
  "tdiv": {
    "id": "xray_render_dmg_indicator",
    "size": "@sizeDamageIndicator, @sizeDamageIndicator",
    "position": "absolute"
  },
  "tdiv": {
    "id": "hud_kill_log",
    "position": "absolute",
    "pos": "pw -w, ph -h -1@hudMisObjIconsSize -1@blockInterval",
    "height": "0.2*@shHud",
    "overflow": "hidden",
    "hudMessages": {
      "id": "hud_message_kill_log_notification",
      "message_style": "kill_log_notification",
      "position": "relative",
      "pos": "pw - w, ph - h",
      "width": "0.5@shHud",
      "flow": "vertical"
    }
  },
  "hitCamera": {
    "id": "hud_hitcamera",
    "position": "absolute",
    "pos": "pw - w, 0",
    "result": "",
    "_transp-timer": "0",
    "hitCameraRender": {
      "hitCameraTitle": {
        "id": "title"
      },
      "hitCameraCrew": {
        "id": "crew_nest",
        "pos": "1@blockInterval, 0.2ph",
        "position": "absolute",
        "img": {
          "size": "1@cIco, 1@cIco",
          "pos": "0, ph-h",
          "position": "relative",
          "margin-left": "1@blockInterval",
          "background-image": "#ui/gameuiskin#ship_crew.svg",
          "background-svg-size": "1@cIco, 1@cIco"
        },
        "tdiv": {
          "pos": "0, ph-h",
          "position": "relative",
          "hitCameraLostCrew": {
            "id": "lost_crew_count",
            "pos": "pw-w, -h",
            "position": "absolute",
            "flow": "vertical"
          },
          "textareaNoTab": {
            "id": "crew_count"
          }
        },
        "textareaNoTab": {
          "id": "max_crew_count",
          "pos": "0, ph-h",
          "position": "relative",
          "overlayTextColor": "active"
        }
      },
      "enemyDamageStatusPanel": {
        "flow": "vertical",
        "tdiv": {
          "id": "damageStatus",
          "pos": "0.5pw-0.5w, 0",
          "position": "relative"
        }
      },
      "hitCameraInfo": {
        "id": "info"
      },
      "tdiv": {
        "size": "pw, ph",
        "position": "absolute",
        "overflow": "hidden",
        "hitCameraCritAnim": {
          "id": "critAnim",
          "height": "1@hitCameraHeight",
          "needAnim": "no",
          "_size-timer": "0",
          "_transp-timer": "0",
          "color-factor": "255"
        }
      }
    }
  },
  "tdiv": {
    "behaviour": "darg",
    "position": "root",
    "pos": "0, 0",
    "size": "sw, sh"
  },
  "hudActionBar": {
    "id": "hud_action_bar",
    "position": "root",
    "pos": "sw/2 - w/2, @actionBarBottomPos - h",
    "tdiv": {
      "id": "actions_nest"
    }
  },
  "tdiv": {
    "position": "absolute",
    "pos": "pw/2-w/2, 0",
    "width": "@shHud",
    "flow": "vertical",
    "multiplayerScore": {
      "id": "hud_multiplayer_score",
      "behaviour": "bhvUpdateByWatched",
      "left": "50%pw-50%w",
      "top": "0.065@scrn_tgt",
      "position": "relative",
      "height": "1@multiplayerScoreHeight",
      "tdiv": {
        "id": "hud_multiplayer_score_progress_bar",
        "size": "11ph, ph",
        "position": "absolute",
        "pos": "50%pw-50%w, 0"
      }
    },
    "hudMessages": {
      "id": "hud_messages_race_messages",
      "message_style": "race_notification",
      "position": "absolute",
      "pos": "pw/2 - w/2, 0.05@shHud",
      "width": "pw",
      "flow": "vertical",
      "text": {
        "id": "race_status",
        "pos": "50%pw - 50%w, 0",
        "position": "relative",
        "overlayTextColor": "active",
        "text": "",
        "hideEmptyText": "yes",
        "text-align": "center",
        "hudFont": "small"
      },
      "tdiv": {
        "id": "leader",
        "pos": "50%pw - 50%w, 0",
        "position": "relative",
        "isPlayer": "no",
        "isLeader": "yes",
        "tdiv": {
          "pos": "50%pw-75%w, 0",
          "position": "relative",
          "width": "0.1@shHud + 0.05@shHud",
          "text": {
            "id": "time",
            "width": "0.1@shHud",
            "text": "",
            "hideEmptyText": "yes",
            "text-align": "right",
            "hudFont": "small"
          },
          "text": {
            "id": "place",
            "width": "0.05@shHud",
            "text": "",
            "hideEmptyText": "yes",
            "text-align": "center",
            "hudFont": "small"
          }
        },
        "text": {
          "id": "name",
          "text": "",
          "hideEmptyText": "yes",
          "position": "relative",
          "hudFont": "small"
        }
      },
      "tdiv": {
        "id": "beforePlayer",
        "pos": "50%pw - 50%w, 0",
        "position": "relative",
        "isPlayer": "no",
        "tdiv": {
          "pos": "50%pw-75%w, 0",
          "position": "relative",
          "width": "0.1@shHud + 0.05@shHud",
          "text": {
            "id": "time",
            "width": "0.1@shHud",
            "text": "",
            "hideEmptyText": "yes",
            "text-align": "right",
            "hudFont": "small"
          },
          "text": {
            "id": "place",
            "width": "0.05@shHud",
            "text": "",
            "hideEmptyText": "yes",
            "text-align": "center",
            "hudFont": "small"
          }
        },
        "text": {
          "id": "name",
          "text": "",
          "hideEmptyText": "yes",
          "position": "relative",
          "hudFont": "small"
        }
      },
      "tdiv": {
        "id": "player",
        "pos": "50%pw - 50%w, 0",
        "position": "relative",
        "isPlayer": "no",
        "isLeader": "yes",
        "tdiv": {
          "pos": "50%pw-75%w, 0",
          "position": "relative",
          "width": "0.1@shHud + 0.05@shHud",
          "text": {
            "id": "time",
            "width": "0.1@shHud",
            "text": "",
            "hideEmptyText": "yes",
            "text-align": "right",
            "hudFont": "small"
          },
          "text": {
            "id": "place",
            "width": "0.05@shHud",
            "text": "",
            "hideEmptyText": "yes",
            "text-align": "center",
            "hudFont": "small"
          }
        },
        "text": {
          "id": "name",
          "text": "",
          "hideEmptyText": "yes",
          "position": "relative",
          "hudFont": "small"
        },
        "hudMessages": {
          "id": "hud_messages_race_bonus_time",
          "message_style": "player_damage_notification",
          "margin-left": "0.02@shHud",
          "flow": "vertical"
        }
      },
      "tdiv": {
        "id": "afterPlayer",
        "pos": "50%pw - 50%w, 0",
        "position": "relative",
        "isPlayer": "no",
        "tdiv": {
          "pos": "50%pw-75%w, 0",
          "position": "relative",
          "width": "0.1@shHud + 0.05@shHud",
          "text": {
            "id": "time",
            "width": "0.1@shHud",
            "text": "",
            "hideEmptyText": "yes",
            "text-align": "right",
            "hudFont": "small"
          },
          "text": {
            "id": "place",
            "width": "0.05@shHud",
            "text": "",
            "hideEmptyText": "yes",
            "text-align": "center",
            "hudFont": "small"
          }
        },
        "text": {
          "id": "name",
          "text": "",
          "hideEmptyText": "yes",
          "position": "relative",
          "hudFont": "small"
        }
      }
    },
    "tdiv": {
      "pos": "pw/2 - w/2, 1@topCenterMessagesTopPos",
      "position": "absolute",
      "flow": "vertical",
      "tdiv": {
        "min-height": "1@topCenterMainMessagesMinHeight",
        "position": "relative",
        "pos": "pw/2 - w/2, 0",
        "flow": "vertical",
        "animSizeObj": {
          "id": "hud_streaks",
          "position": "relative",
          "pos": "pw/2 - w/2, 0",
          "width": "45%@rwHud",
          "animation": "hide",
          "height-base": "1",
          "height-end": "100",
          "height": "1",
          "_size-timer": "0",
          "tdiv": {
            "id": "streak_content",
            "width": "pw",
            "flow": "vertical",
            "display": "hide",
            "enable": "no",
            "behaviour": "massTransparency",
            "transp-base": "255",
            "transp-end": "0",
            "transp-func": "cube",
            "transp-time": "2000",
            "_transp-timer": "1",
            "_blink": "no",
            "img": {
              "id": "streak_icon",
              "size": "0.06@shHud, 0.06@shHud",
              "pos": "pw/2-w/2, 0",
              "position": "relative"
            },
            "textareaNoTab": {
              "id": "streak_header",
              "width": "pw",
              "style": "color:@streakTextColor; font:@fontHudMedium; text-align:center;",
              "text": "",
              "shadeStyle": "HudHeader3D",
              "pos": "0, -0.004*@shHud",
              "position": "relative"
            },
            "textareaNoTab": {
              "id": "streak_score",
              "width": "pw",
              "style": "color:@white; font:@fontHudSmall; text-align:center;",
              "shadeStyle": "HudTinyLight",
              "text": ""
            }
          }
        },
        "hudMessages": {
          "id": "hud_message_center_main_notification",
          "pos": "pw/2 - w/2, 0",
          "position": "relative",
          "message_style": "center_main_notification",
          "display": "hide"
        }
      },
      "hudMessages": {
        "id": "hud_messages_death_reason_notification",
        "width": "@shHud",
        "pos": "pw/2 - w/2, 0",
        "position": "relative",
        "flow": "vertical",
        "message_style": "death_reason_notification"
      },
      "hudMessages": {
        "id": "hud_messages_reward_messages",
        "width": "@shHud",
        "position": "relative",
        "pos": "pw/2 - w/2, 0",
        "flow": "vertical",
        "message_style": "reward_notification",
        "message": {
          "id": "reward_message",
          "css-hier-invalidate": "yes",
          "position": "relative",
          "pos": "pw/2 - w/2",
          "padding": "0.2@shHud, 0",
          "display": "hide",
          "enable": "no",
          "behaviour": "massTransparency",
          "transp-base": "255",
          "transp-end": "0",
          "transp-func": "square",
          "transp-time": "2000",
          "blend-time": "0",
          "rewardMessageText": {
            "id": "reward_message_text",
            "position": "relative",
            "text": "",
            "view_class": ""
          },
          "rewardMessageValueContainer": {
            "id": "reward_value_container",
            "position": "absolute",
            "pos": "pw/2 - w/2, ph - h/2 + 0.01@shHud",
            "css-hier-invalidate": "yes",
            "_blink": "no",
            "rewardMessageValue": {
              "id": "reward_total",
              "position": "absolute",
              "pos": "pw/2-w/2, ph/2-h/2",
              "text": ""
            }
          }
        }
      }
    },
    "hudMessages": {
      "id": "hud_message_center_mission_result",
      "pos": "50%pw-50%w, 0.4@shHud",
      "position": "absolute",
      "message_style": "mission_result_notification"
    },
    "tdiv": {
      "id": "mission_result_box_start",
      "height": "0.3@sf",
      "width": "10h",
      "position": "absolute",
      "pos": "50%pw-50%w, 0.3@shHud - 50%h"
    }
  },
  "tdiv": {
    "id": "tutorial_elements_nest",
    "size": "@rwHud, @rhHud",
    "pos": "pw/2-w/2, ph/2-h/2",
    "position": "absolute",
    "display": "hide",
    "total-input-transparent": "yes"
  },
  "tdiv": {
    "id": "hintBlock",
    "behaviour": "bhvUpdateByWatched",
    "pos": "0.5pw - 0.5w, ph - h",
    "position": "absolute",
    "tdiv": {
      "id": "hud_hints_nest",
      "width": "2/3@rwHud",
      "flow": "vertical",
      "css-hier-invalidate": "yes",
      "behavior": "Timer",
      "tdiv": {
        "width": "pw",
        "position": "absolute",
        "pos": "pw/2 - w/2, -h",
        "flow": "vertical",
        "tdiv": {
          "id": "common_priority_hints",
          "width": "pw",
          "flow": "vertical"
        },
        "tdiv": {
          "id": "ship_obstacle_rf",
          "size": "pw, 0.04@shHud"
        },
        "tdiv": {
          "width": "pw",
          "min-height": "0.1@shHud",
          "tdiv": {
            "id": "mission_hints",
            "width": "pw",
            "position": "relative",
            "pos": "0, ph-h",
            "flow": "vertical"
          }
        },
        "tdiv": {
          "id": "mission_action_hints_holder",
          "position": "relative",
          "tdiv": {
            "id": "mission_action_hints",
            "width": "pw",
            "position": "relative",
            "pos": "0, ph-h",
            "flow": "vertical"
          }
        }
      },
      "hudMessages": {
        "id": "hud_message_zone_capture_notification",
        "width": "pw",
        "height": "0.065@shHud",
        "pos": "0.5pw-0.5w, -@tankGunsAmmoBlockHeight",
        "message_style": "zone_capture_notification",
        "input-transparent": "yes"
      },
      "tdiv": {
        "height": "0.08@shHud",
        "position": "relative",
        "left": "pw/2 - w/2",
        "div": {
          "id": "iconed_hints",
          "height": "ph"
        },
        "div": {
          "id": "display_timers",
          "height": "ph"
        }
      },
      "tdiv": {
        "id": "actionbar_hints_nest",
        "width": "1@shHud",
        "height": "0.065@shHud",
        "left": "0.5pw-0.5w",
        "position": "relative",
        "tdiv": {
          "width": "pw",
          "position": "absolute",
          "pos": "0, ph-h",
          "flow": "vertical",
          "tdiv": {
            "id": "actionbar_hints",
            "width": "pw",
            "flow": "vertical"
          }
        }
      },
      "hudMessages": {
        "id": "hud_message_player_damage_notification",
        "message_style": "player_damage_notification",
        "left": "0.2@shHud",
        "position": "relative",
        "flow": "vertical",
        "width": "pw - 0.2@shHud"
      },
      "textAreaCentered": {
        "id": "low-quality-model-warning",
        "width": "pw",
        "style": "color:@warningTextColor; font:@fontHudNormal;",
        "text": "#low_quality_model_in_hangar",
        "display": "hide"
      },
      "tdiv": {
        "id": "tutorial_hints",
        "width": "pw",
        "position": "root",
        "pos": "50%sw-50%w, 1@bhHud + 0.67@shHud",
        "flow": "vertical"
      },
      "tdiv": {
        "id": "minor_priority_hints",
        "width": "pw",
        "position": "root",
        "pos": "50%sw-50%w, @shHud - h",
        "flow": "vertical"
      }
    }
  },
  "tdiv": {
    "id": "hud_live_stats_nest",
    "width": "@shHud",
    "pos": "pw/2-w/2, ph-h-0.03@shHud",
    "position": "absolute",
    "display": "hide"
  },
  "tdiv": {
    "id": "hud_message_timers"
  },
  "timer": {
    "id": "hud_message_timer"
  }
