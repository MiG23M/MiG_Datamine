{
  "root": {
    "blur": {    },
    "blur_foreground": {    },
    "gamercardShadePlace": {
      "gamercardShade": {      }
    },
    "frame": {
      "id": "reward_frame",
      "width": "1@trophyWndWidth",
      "class": "wndNav",
      "type": "big",
      "isCenteredUnderLogo": "yes",
      "frame_header": {
        "textareaNoTab": {
          "id": "reward_title",
          "caption": "yes",
          "overlayTextColor": "active",
          "text": ""
        },
        "Button_close": {
          "id": "btn_back"
        }
      },
      "tdiv": {
        "width": "pw",
        "flow": "horizontal",
        "flow-align": "center",
        "div": {
          "id": "reward_image_place",
          "flow": "vertical",
          "flow-align": "center",
          "position": "relative",
          "pos": "0, ph/2-h/2",
          "size": "@chestRewardHeight, @chestRewardHeight",
          "display": "hide",
          "class": "button",
          "_on_click": "onViewRewards",
          "total-input-transparent": "yes"
        },
        "textareaNoTab": {
          "id": "extended_desc",
          "position": "relative",
          "pos": "0, ph/2-h/2",
          "padding-top": "1@blockInterval",
          "padding-bottom": "3@blockInterval",
          "width": "fw",
          "hideEmptyText": "yes",
          "smallFont": "yes",
          "text": "",
          "display": "hide"
        }
      },
      "img": {
        "id": "reward_image",
        "class": "button",
        "position": "relative",
        "width": "pw",
        "height": "1@chestRewardHeight",
        "display": "hide",
        "background-repeat": "aspect-ratio",
        "_on_click": "onViewRewards"
      },
      "tdiv": {
        "id": "open_chest_animation",
        "pos": "50%pw-50%w, 1@rewardInTrophyWndTopPos + 1@frameHeaderHeight",
        "size": "1@chestRewardWidth, 1@chestRewardHeight",
        "position": "absolute",
        "overflow": "visible",
        "behaviour": "Timer",
        "animation": "hide",
        "css-hier-invalidate": "yes",
        "chestReplaceDelay": "450",
        "openAnim": {
          "pos": "50%pw-50%w, 30%ph-50%h",
          "position": "absolute",
          "background-image": "#ui/images/chests/open_flash",
          "background-color": "#10FFFFFF",
          "color-factor": "0",
          "transp-base": "0",
          "transp-end": "255",
          "transp-func": "blinkSin",
          "transp-time": "650",
          "width": "0.1pw",
          "height": "0.1ph",
          "width-base": "30",
          "height-base": "30",
          "width-end": "300",
          "height-end": "300",
          "size-func": "squareInv",
          "size-scale": "parent",
          "size-time": "650",
          "rot-base": "0",
          "rot-end": "360",
          "rot-func": "square",
          "rot-time": "650"
        },
        "openAnim": {
          "pos": "50%pw-50%w, 30%ph-50%h",
          "position": "absolute",
          "background-image": "#ui/images/chests/open_flash",
          "background-color": "#10FFFFFF",
          "color-factor": "0",
          "transp-base": "0",
          "transp-end": "255",
          "transp-func": "blinkCos",
          "transp-time": "650",
          "width": "0.1pw",
          "height": "0.1ph",
          "width-base": "30",
          "height-base": "30",
          "width-end": "300",
          "height-end": "300",
          "size-func": "squareInv",
          "size-scale": "parent",
          "size-time": "650",
          "rot-base": "90",
          "rot-end": "-135",
          "rot-func": "squareInv",
          "rot-time": "650"
        },
        "openAnim": {
          "pos": "50%pw-50%w, 30%ph-50%h",
          "position": "absolute",
          "background-image": "#ui/images/chests/open_flash_dots",
          "background-color": "#10FFFFFF",
          "on_deactivate": "onOpenAnimFinish",
          "color-factor": "0",
          "transp-base": "0",
          "transp-end": "255",
          "transp-func": "blink",
          "transp-time": "650",
          "width": "0.1pw",
          "height": "0.1ph",
          "width-base": "30",
          "height-base": "30",
          "width-end": "320",
          "height-end": "320",
          "size-func": "squareInv",
          "size-scale": "parent",
          "size-time": "650"
        }
      },
      "textareaNoTab": {
        "id": "reward_postscript",
        "pos": "pw - w - 1@trophyWndPad, 1@frameHeaderHeight + 1@rewardInTrophyWndTopPos + 0.04ph + 1@itemHeight",
        "position": "absolute",
        "text": ""
      },
      "massTransp": {
        "id": "reward_roullete",
        "pos": "50%pw-50%w, 1@frameHeaderHeight + 0.02@scrn_tgt",
        "position": "absolute",
        "type": "roulette",
        "behaviour": "Timer",
        "animation": "hide",
        "height": "1@itemHeight",
        "width": "4@itemWidth",
        "max-width": "pw",
        "css-hier-invalidate": "yes",
        "totalLen": "45",
        "insertRewardFromEnd": "2",
        "roulette": {
          "size": "pw, ph",
          "padding": "1@framePadding, 0",
          "overflow": "hidden",
          "css-hier-invalidate": "yes",
          "tdiv": {
            "pos": "50%pw-50%w, 0",
            "position": "relative",
            "height": "ph",
            "css-hier-invalidate": "yes",
            "roulette_scroll": {
              "id": "rewards_list",
              "height": "ph",
              "position": "absolute"
            },
            "tdiv": {
              "height": "ph",
              "width": "2",
              "pos": "50%pw-50%w, 50%ph-50%h",
              "position": "absolute",
              "received_reward_indicator": {
                "pos": "50%pw-50%w, 2",
                "position": "absolute",
                "rotation": "180"
              },
              "received_reward_indicator": {
                "pos": "50%pw-50%w, ph -h -1",
                "position": "absolute"
              }
            }
          }
        },
        "rewards_fade": {
          "pos": "2, 3",
          "position": "absolute",
          "size": "50%pw-0.5@itemWidth, ph-6"
        },
        "rewards_fade": {
          "pos": "pw-w-2, 3",
          "position": "absolute",
          "rotation": "180",
          "size": "50%pw-0.5@itemWidth, ph-6"
        }
      },
      "tdiv": {
        "width": "pw",
        "min-height": "0.1@sf",
        "max-height": "@rh - 1@chestRewardHeight - 1@frameHeaderHeight - 1@frameFooterHeight - 0.2@scrn_tgt",
        "pos": "0, -0.03@sf",
        "position": "relative",
        "flow": "vertical",
        "overflow-y": "auto",
        "scrollbarShortcuts": "yes",
        "textarea": {
          "id": "prize_desc_text",
          "max-width": "pw",
          "pos": "50%pw-50%w, 0.03@sf",
          "position": "relative",
          "text": "",
          "text-align": "center"
        },
        "tdiv": {
          "width": "pw-2@blockInterval",
          "pos": "pw/2-w/2, 0",
          "position": "relative",
          "tdiv": {
            "id": "prize_desc_div",
            "max-width": "pw",
            "pos": "50%pw-50%w, 0",
            "position": "relative"
          }
        }
      },
      "navBar": {
        "navLeft": {
          "Button_text": {
            "id": "btn_rewards_list",
            "text": "#mainmenu/rewardsList",
            "btnName": "Y",
            "_on_click": "onViewRewards",
            "ButtonImg": {            }
          }
        },
        "navRight": {
          "Button_text": {
            "id": "btn_go_to_item",
            "text": "#item/goToItem",
            "btnName": "X",
            "_on_click": "onGoToItem",
            "display": "hide",
            "enable": "no",
            "ButtonImg": {            }
          },
          "Button_text": {
            "id": "btn_re_use_item",
            "text": "",
            "btnName": "RB",
            "_on_click": "onReUseItem",
            "display": "hide",
            "enable": "no",
            "ButtonImg": {            }
          },
          "Button_text": {
            "id": "btn_take_air",
            "btnName": "X",
            "text": "#mainmenu/btnTakeAircraft",
            "class": "battle",
            "on_click": "onTakeNavBar",
            "navButtonFont": "yes",
            "css-hier-invalidate": "yes",
            "display": "hide",
            "enable": "no",
            "pattern": {            },
            "buttonWink": {
              "_transp-timer": "0"
            },
            "buttonGlance": {            },
            "ButtonImg": {            },
            "btnText": {
              "id": "btn_set_air_text",
              "text": "#mainmenu/btnTakeAircraft"
            }
          },
          "Button_text": {
            "id": "btn_use_decorator",
            "text": "#decorator/use/decal",
            "btnName": "X",
            "_on_click": "onUseDecorator",
            "display": "hide",
            "enable": "no",
            "ButtonImg": {            }
          },
          "Button_text": {
            "id": "btn_preview_decorator",
            "text": "#mainmenu/btnPreview",
            "btnName": "X",
            "_on_click": "onPreviewDecorator",
            "display": "hide",
            "enable": "no",
            "showButtonImageOnConsole": "no",
            "class": "image",
            "img": {
              "background-image": "#ui/gameuiskin#btn_preview.svg"
            },
            "ButtonImg": {            }
          },
          "Button_text": {
            "id": "btn_preloader_settings",
            "text": "#preloaderSettings/title",
            "btnName": "X",
            "_on_click": "onPreloaderSettings",
            "display": "hide",
            "enable": "no",
            "ButtonImg": {            }
          },
          "Button_text": {
            "id": "btn_run_custom_mission",
            "text": "",
            "btnName": "X",
            "_on_click": "onRunCustomMission",
            "display": "hide",
            "enable": "no",
            "ButtonImg": {            }
          },
          "Button_text": {
            "id": "btn_ok",
            "text": "#mainmenu/btnOk",
            "btnName": "A",
            "_on_click": "goBack",
            "ButtonImg": {            }
          }
        }
      }
    }
  },
  "timer": {
    "id": "update_timer",
    "timer_handler_func": "onTimer",
    "timer_interval_msec": "1000"
  }
}