{
  "root": {
    "type": "big",
    "blur": {    },
    "blur_foreground": {    },
    "frame": {
      "width": "1@itemWidth + 2@itemSpacing + 1@blockInterval + 1@itemInfoWidth + 2@framePadding",
      "pos": "50%pw-50%w, 50%ph-50%h",
      "position": "absolute",
      "class": "wnd",
      "frame_header": {
        "activeText": {
          "id": "title",
          "text": "#mainmenu/rewardsList",
          "caption": "yes"
        },
        "Button_close": {
          "id": "btn_back"
        }
      },
      "tdiv": {
        "id": "trophy_rewards_list",
        "width": "pw",
        "height": "4@itemHeight+ 5@itemSpacing + 2@framePadding",
        "max-height": "1@maxWindowHeight",
        "tdiv": {
          "id": "items_list",
          "width": "1@itemWidth + 2@itemSpacing",
          "flow": "vertical",
          "overflow-y": "auto",
          "itemShopList": "yes",
          "bringFocusBorderToFront": "no",
          "behavior": "posNavigator",
          "navigatorShortcuts": "yes",
          "moveX": "closest",
          "moveY": "linear",
          "clearOnFocusLost": "no",
          "total-input-transparent": "yes",
          "on_select": "updateItemInfo"
        },
        "chapterSeparator": {
          "margin": "1@blockInterval, 0, 0, 0"
        },
        "tdiv": {
          "size": "fw, ph",
          "pos": "1@blockInterval, 0",
          "flow": "vertical",
          "padding": "0.01@scrn_tgt",
          "overflow-y": "auto",
          "scrollbarShortcuts": "yes",
          "tdiv": {
            "id": "item_info",
            "width": "pw",
            "display": "hide",
            "flow": "vertical"
          },
          "tdiv": {
            "id": "prize_info",
            "width": "pw",
            "text-align": "center",
            "flow": "vertical"
          }
        }
      }
    }
  }
}