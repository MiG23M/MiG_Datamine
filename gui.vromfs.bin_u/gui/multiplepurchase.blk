{
  "root": {
    "type": "big",
    "blur": {    },
    "blur_foreground": {    },
    "frame": {
      "width": "0.7@sf",
      "class": "wndNav",
      "isCenteredUnderLogo": "yes",
      "frame_header": {
        "activeText": {
          "id": "item_name_header",
          "text": "#shop/multiplePurchaseHeader",
          "caption": "yes",
          "pare-text": "yes",
          "width": "pw-1@cIco-16"
        },
        "Button_close": {        }
      },
      "tdiv": {
        "size": "pw, 1@modCellHeight",
        "tdiv": {
          "id": "icon",
          "size": "1@modCellWidth, 1@modCellHeight"
        },
        "tdiv": {
          "width": "fw",
          "pos": "0, ph -h -1@modPadSize",
          "position": "relative",
          "Button_text": {
            "id": "buttonDec",
            "text": "-",
            "square": "yes",
            "on_click": "onButtonDec",
            "tooltip": "#weaponry/descreaseValue"
          },
          "invisSlider": {
            "id": "skillSlider",
            "size": "fw, 2*@scrn_tgt/100.0",
            "pos": "0, 50%ph-50%h",
            "position": "relative",
            "margin": "0.5@sliderThumbWidth + 1@blockInterval, 0",
            "value": "0",
            "min": "0",
            "max": "100",
            "on_change_value": "onProgressChanged",
            "total-input-transparent": "yes",
            "expProgress": {
              "id": "newSkillProgress",
              "width": "pw",
              "pos": "50%pw-50%w, 50%ph-50%h",
              "position": "absolute",
              "type": "new",
              "value": "70"
            },
            "expProgress": {
              "id": "oldSkillProgress",
              "width": "pw",
              "pos": "50%pw-50%w, 50%ph-50%h",
              "position": "absolute",
              "type": "old",
              "value": "30"
            },
            "focus_border": {            },
            "sliderButton": {
              "id": "weaponry_slider_button",
              "type": "various",
              "img": {              },
              "activeText": {
                "id": "text_buyingValue",
                "pos": "50%pw-50%w, -h",
                "position": "absolute",
                "talign": "left",
                "valign": "center",
                "overlayTextColor": "userlog",
                "input-transparent": "yes"
              }
            }
          },
          "Button_text": {
            "id": "buttonInc",
            "text": "+",
            "square": "yes",
            "on_click": "onButtonInc",
            "tooltip": "#weaponry/increaseValue"
          }
        }
      },
      "navBar": {
        "navLeft": {
          "Button_text": {
            "id": "buttonMax",
            "text": "#profile/maximumExp",
            "on_click": "onButtonMax",
            "tooltip": "#shop/setMaxValue",
            "btnName": "Y",
            "ButtonImg": {            }
          }
        },
        "navRight": {
          "Button_text": {
            "id": "item_price",
            "on_click": "onBuy",
            "hideText": "yes",
            "btnName": "X",
            "visualStyle": "purchase",
            "buttonWink": {            },
            "buttonGlance": {            },
            "textarea": {
              "id": "item_price_text",
              "class": "buttonText"
            },
            "discount": {
              "id": "multPurch_discount",
              "text": "-300%",
              "pos": "pw-30%w, -50%h",
              "position": "absolute",
              "rotation": "-30"
            },
            "ButtonImg": {            }
          }
        }
      }
    }
  }
}