{
  "flow": "vertical",
  "tdiv": {
    "width": "pw",
    "flow": "horizontal",
    "padding-bottom": "1@blockInterval",
    "HorizontalListBox": {
      "id": "modes_list",
      "type": "transparent",
      "navigatorShortcuts": "yes",
      "move-only-hover": "yes",
      "on_select": "onStatsModeChange"
    },
    "popupFilter": {
      "left": "pw-w",
      "position": "absolute"
    }
  },
  "tdiv": {
    "size": "pw, fh",
    "position": "relative",
    "pos": "0.5pw-0.5w, 0",
    "position": "relative",
    "table": {
      "id": "airs_stats_table",
      "size": "pw, ph",
      "pos": "0.5pw-0.5w, 0",
      "position": "relative",
      "class": "lbTable",
      "higlightRowOnHover": "yes",
      "text-valign": "center",
      "text-halign": "center"
    }
  },
  "animated_wait_icon": {
    "id": "stats_loading",
    "pos": "50%pw-50%w, 50%ph-50%h",
    "position": "absolute",
    "background-rotation": "0"
  }
}