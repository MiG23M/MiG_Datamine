{
  "selImg": {
    "id": "unlock_block",
    "width": "pw",
    "mainDescWrap": {
      "padding": "1.5*@sf/100.0",
      "width": "pw",
      "layeredIconContainer": {
        "id": "achivment_ico",
        "size": "@profileUnlockIconSize, @profileUnlockIconSize",
        "position": "relative",
        "pos": "0, ph/2 - h/2"
      },
      "achievementTitle": {
        "margin-left": "0.015@sf",
        "min-height": "@profileUnlockIconSize",
        "width": "fw",
        "flow": "vertical",
        "activeText": {
          "id": "achivment_title",
          "text": "",
          "max-width": "pw",
          "pare-text": "yes",
          "overlayTextColor": "unlockHeader"
        },
        "textareaNoTab": {
          "id": "description",
          "width": "pw",
          "overflow": "hidden",
          "smallFont": "yes",
          "text": ""
        },
        "challengeDescriptionProgress": {
          "id": "progress_bar",
          "value": "",
          "display": "hide"
        },
        "textareaNoTab": {
          "id": "main_cond",
          "width": "pw",
          "overflow": "hidden",
          "smallFont": "yes",
          "text": "",
          "margin-top": "1@blockInterval"
        },
        "textareaNoTab": {
          "id": "mult_desc",
          "width": "pw",
          "overflow": "hidden",
          "smallFont": "yes",
          "text": ""
        },
        "textareaNoTab": {
          "id": "conditions",
          "width": "pw",
          "overflow": "hidden",
          "smallFont": "yes",
          "text": "",
          "margin-top": "1@blockInterval"
        },
        "textareaNoTab": {
          "id": "reward",
          "text": "",
          "smallFont": "yes",
          "margin-top": "0.005@sf",
          "title": "$tooltipObj",
          "max-width": "pw",
          "pare-text": "yes",
          "tooltipObj": {
            "id": "tooltip",
            "reward": "",
            "smallFont": "yes",
            "on_tooltip_open": "onGenericTooltipOpen",
            "on_tooltip_close": "onTooltipObjClose",
            "display": "hide"
          }
        },
        "tdiv": {
          "width": "pw",
          "left": "-1@blockInterval",
          "position": "relative",
          "Button_text": {
            "id": "purchase_button",
            "hideText": "yes",
            "visualStyle": "purchase",
            "enableOn": "hoverOrSelect",
            "on_click": "onBuyUnlock",
            "unlockId": "",
            "btnName": "Y",
            "buttonWink": {            },
            "ButtonImg": {
              "showOnSelect": "yes"
            },
            "textarea": {
              "id": "purchase_button_text",
              "class": "buttonText"
            }
          },
          "Button_text": {
            "id": "manual_open_button",
            "visualStyle": "purchase",
            "enableOn": "hoverOrSelect",
            "on_click": "onManualOpenUnlock",
            "unlockId": "",
            "btnName": "Y",
            "text": "#items/getRewardShort",
            "buttonWink": {            },
            "ButtonImg": {
              "showOnSelect": "yes"
            }
          },
          "Button_text": {
            "id": "show_prizes_btn",
            "trophyId": "",
            "text": "#profile/awards",
            "margin-top": "1@blockInterval",
            "margin-bottom": "1@blockInterval",
            "btnName": "L3",
            "enableOn": "hoverOrSelect",
            "display": "hide",
            "on_click": "showUnlockPrizes",
            "visualStyle": "secondary",
            "buttonWink": {            },
            "ButtonImg": {
              "showOnSelect": "yes"
            }
          },
          "Button_text": {
            "id": "preview_prize_btn",
            "unlockId": "",
            "text": "#mainmenu/btnPreview",
            "margin-top": "1@blockInterval",
            "margin-bottom": "1@blockInterval",
            "btnName": "L3",
            "enableOn": "hoverOrSelect",
            "display": "hide",
            "on_click": "onPrizePreview",
            "visualStyle": "secondary",
            "class": "image",
            "buttonWink": {            },
            "img": {
              "background-image": "#ui/gameuiskin#btn_preview.svg"
            },
            "ButtonImg": {
              "showOnSelect": "yes"
            }
          },
          "Button_text": {
            "id": "show_units_btn",
            "unlockId": "",
            "text": "#mainmenu/showVehicles",
            "margin-top": "1@blockInterval",
            "margin-bottom": "1@blockInterval",
            "btnName": "X",
            "enableOn": "hoverOrSelect",
            "display": "hide",
            "on_click": "showUnlockUnits",
            "visualStyle": "secondary",
            "buttonWink": {            },
            "ButtonImg": {
              "showOnSelect": "yes"
            }
          }
        }
      },
      "unlockStages": {
        "id": "stages",
        "margin-left": "0.05@sf",
        "pos": "0, 50%ph-50%h",
        "position": "relative",
        "flow": "horizontal",
        "behavior": "PosNavigator",
        "navigatorShortcuts": "noSelect"
      },
      "CheckBox": {
        "id": "checkbox_favorites",
        "pos": "pw-w-@blockInterval, @blockInterval",
        "position": "absolute",
        "text": "#mainmenu/UnlockAchievementsToFavorite",
        "smallFont": "yes",
        "btnName": "",
        "tooltip": "",
        "skip-navigation": "yes",
        "on_change_value": "unlockToFavorites",
        "unlockId": "",
        "CheckBoxImg": {        },
        "ButtonImg": {
          "showOnSelect": "hover",
          "btnName": "A"
        }
      }
    },
    "expandImg": {
      "id": "expandImg",
      "height": "1*@scrn_tgt/100.0",
      "width": "2h",
      "pos": "50%pw-50%w, ph-h-0.7*@sf/100.0",
      "position": "absolute",
      "background-image": "#ui/gameuiskin#expand_info",
      "background-color": "@premiumColor",
      "display": "hide"
    },
    "hiddenDiv": {
      "id": "hidden_block",
      "width": "pw",
      "padding": "8*@scrn_tgt/100.0, 0, 1*@scrn_tgt/100.0, 0",
      "flow": "h-flow"
    },
    "tdiv": {
      "pos": "10@sf/@pf, 10@sf/@pf",
      "position": "absolute",
      "height": "@newWidgetIconHeight",
      "unseenIcon": {
        "id": "unseen_icon",
        "noMargin": "yes"
      },
      "img": {
        "id": "lock_icon",
        "display": "hide",
        "pos": "0, 50%(ph-h)",
        "position": "relative",
        "background-image": "#ui/gameuiskin#locked.svg",
        "size": "@cIco,@cIco",
        "background-svg-size": "@cIco,@cIco",
        "background-color": "@white"
      }
    }
  }
}