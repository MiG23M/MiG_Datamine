        "textarea": {
          "id": "btn_repairall_text",
          "class": "buttonText",
          "text": "#mainmenu/btnRepairAll"
        }
      },
      "Button_text": {
        "id": "btn_spend_exp",
        "navButtonFont": "yes",
        "display": "hide",
