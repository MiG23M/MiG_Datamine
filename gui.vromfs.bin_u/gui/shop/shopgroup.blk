    "navLeft": {
      "tdiv": {
        "height": "1@buttonHeight",
        "textareaNoTab": {
          "id": "excess_exp_text",
          "caption": "yes",
          "pos": "1@buttonMargin, ph/2-h/2",
          "position": "absolute"
        }
      }
    },
    "navRight": {
      "Button_text": {
        "id": "btn_repairall",
        "hideText": "yes",
        "btnName": "L3",
