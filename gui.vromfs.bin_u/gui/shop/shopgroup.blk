        "css-hier-invalidate": "yes",
        "btnName": "X",
        "visualStyle": "secondary",
        "css-hier-invalidate": "yes",
        "buttonWink": {        },
        "textarea": {
          "id": "btn_spend_exp_text",
          "class": "buttonText",
          "text": "#weaponry/research"
        },
        "ButtonImg": {        }
      },
      "Button_text": {
        "id": "btn_buy_unit",
        "navButtonFont": "yes",
