{
  "group": "",
  "DummyButton": {
    "id": "slotb-aircraft-choose-root",
    "size": "sw, sh",
    "position": "root",
    "btnName": "B",
    "on_click": "onCancelSlotChoose",
    "tooltip": "",
    "blur": {    },
    "blur_foreground": {
      "isRoot": "yes"
    }
  },
  "div": {
    "id": "tablePlace",
    "position": "root",
    "behaviour": "basicSize",
    "size-time": "300",
    "size-func": "squareInv",
    "blend-time": "0",
    "css-hier-invalidate": "yes",
    "max-height": "@rh",
    "slotsScrollDiv": {
      "id": "slots_scroll_div",
      "height": "ph",
      "pos": "0, -h-1*@scrn_tgt/100.0",
