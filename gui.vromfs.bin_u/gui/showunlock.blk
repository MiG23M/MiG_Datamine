{
  "root": {
    "type": "big",
    "blur": {    },
    "blur_foreground": {    },
    "frame": {
      "pos": "0.5pw-0.5w, 0.5ph-0.5h",
      "position": "absolute",
      "width": "80%sh",
      "max-width": "800*@sf/@pf_outdated + 2@framePadding",
      "max-height": "@rh",
      "class": "wndNav",
      "frame_header": {
        "textareaNoTab": {
          "id": "award_name",
          "width": "p.p.w-2@buttonHeight",
          "caption": "yes",
          "pare-text": "yes",
          "overlayTextColor": "active"
        },
        "Button_close": {
          "id": "btn_close"
        }
      },
      "img": {
        "id": "award_image",
        "width": "pw",
        "height": "0.75w",
        "max-width": "800*@sf/@pf_outdated",
        "max-height": "600*@sf/@pf_outdated",
        "pos": "50%pw-50%w, 0",
        "position": "relative",
        "background-image": "#ui/images/login_reward?P1",
        "dummy": {
          "id": "award_image_button",
          "link": "",
          "on_click": "onMsgLink",
          "bqKey": "show_unlock_window",
          "size": "pw, ph"
        }
      },
      "tdiv": {
        "width": "pw",
        "max-height": "fh",
        "pos": "0, 1@framePadding",
        "position": "relative",
        "flow": "vertical",
        "overflow-y": "auto",
        "textarea": {
          "id": "award_desc",
          "pos": "50%pw-50%w, 0",
          "position": "relative",
          "max-width": "pw",
          "removeParagraphIndent": "yes",
          "text": "",
          "font-bold": "@fontMedium",
          "on_link_click": "proccessLinkFromText"
        },
        "textarea": {
          "id": "award_reward",
          "pos": "50%pw-50%w, 0",
          "position": "relative",
          "max-width": "pw",
          "overlayTextColor": "userlog",
          "removeParagraphIndent": "yes",
          "text": "",
          "on_link_click": "proccessLinkFromText"
        },
        "tdiv": {
          "id": "reward_markup",
          "pos": "0.5pw-0.5w, 0",
          "position": "relative",
          "flow": "vertical",
          "display": "hide"
        },
        "rankUpList": {
          "id": "reward_aircrafts",
          "max-width": "pw",
          "pos": "0.5pw-0.5w,0",
          "position": "relative",
          "flow": "h-flow",
          "holdTooltipChildren": "yes",
          "on_activate": "onUnitActivate",
          "on_click": "onUnitActivate",
          "on_r_click": "onUnitActivate"
        },
        "textAreaCentered": {
          "id": "next_award",
          "max-width": "pw",
          "pos": "50%pw-50%w, 0.03@sf",
          "position": "relative",
          "textHide": "yes",
          "text": ""
        }
      },
      "navBar": {
        "navLeft": {
          "Button_text": {
            "id": "btn_sendEmail",
            "display": "hide",
            "text": "#mainmenu/btnSendEmail",
            "on_click": "sendInvitation"
          }
        },
        "navRight": {
          "Button_text": {
            "id": "btn_use_decorator",
            "text": "#decorator/use/decal",
            "btnName": "Y",
            "_on_click": "onUseDecorator",
            "enable": "no",
            "display": "hide",
            "ButtonImg": {            }
          },
          "Button_text": {
            "id": "btn_ok",
            "text": "#mainmenu/btnOk",
            "btnName": "X",
            "_on_click": "onOk",
            "ButtonImg": {            }
          }
        }
      }
    }
  }
}