    "pos": "50%pw-50%w, ph-h-1@modBundlePopupBgPadH",
    "position": "absolute",
    "padding": "0, 1@modBundlePopupBgPadH",
    "border": "yes",
    "border-offset": "1@dp",
    "border-color": "@frameDarkMenuBorderColor",
    "background-color": "@frameDarkBackgroundColor",
    "css-hier-invalidate": "yes",
